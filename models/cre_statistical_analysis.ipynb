   "source": [
    "### ARMA Model Forecasting and Plotting ###\n",
    "print(\"--- ARMA Model Forecasting ---\")\n",
    "\n",
    "# Convert pandas Series to Darts TimeSeries\n",
    "series_darts = TimeSeries.from_series(time_series)\n",
    "\n",
    "# Split data into training and validation sets\n",
    "split_fraction = 0.8\n",
    "train_series, val_series = series_darts.split_before(split_fraction)\n",
    "\n",
    "# Instantiate and train the ARMA model\n",
    "arma_model = ARIMA(p=1, d=0, q=1) # ARMA(1,1)\n",
    "arma_model.fit(series=train_series)\n",
    "\n",
    "# Generate predictions\n",
    "arma_prediction = arma_model.historical_forecasts(\n",
    "    val_series,\n",
    "    start=val_series.start_time(),\n",
    "    forecast_horizon=1,\n",
    "    stride=1,\n",
    "    retrain=False,\n",
    "    verbose=False\n",
    ")\n",
    "\n",
    "# Calculate MAE\n",
    "arma_mae = mae(val_series, arma_prediction)\n",
    "print(f\"ARMA Model MAE: {arma_mae:.4f}\")\n",
    "\n",
    "# Plotting\n",
    "plt.figure(figsize=(12, 6))\n",
    "series_darts.plot(label='True Values (Full Series)')\n",
    "train_series.plot(label='Training Data')\n",
    "val_series.plot(label='True Values (Validation)')\n",
    "arma_prediction.plot(label='ARMA Forecast')\n",
    "plt.title(f'ARMA Forecast vs. True Values (MAE: {arma_mae:.4f})')\n",
    "plt.xlabel('Time')\n",
    "plt.ylabel('Value')\n",
    "plt.legend()\n",
    "arma_plot_path = os.path.join(plot_dir, 'cre_arma_forecast.png')\n",
    "plt.savefig(arma_plot_path)\n",
    "plt.close()\n",
    "print(f\"ARMA forecast plot saved to '{arma_plot_path}'\")\n",
    "print(\"\\n\")"
   ]
  },
